{
  "extends": ["next/core-web-vitals", "eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": { "jsx": true },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/no-unknown-property": ["error", { "ignore": ["jsx"] }]
  }
}
